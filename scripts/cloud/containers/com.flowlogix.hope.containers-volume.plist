<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.flowlogix.hope.containers-volume</string>
    <key>ProgramArguments</key>
    <array>
        <string>diskutil</string>
        <string>mount</string>
        <string>/dev/disk7s1</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/Users/software/var/log/containers-volume.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/software/var/log/containers-volume.log</string>
</dict>
</plist>
