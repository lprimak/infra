<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <activeProfiles>
        <activeProfile>payara-arquillian-version-override</activeProfile>
        <activeProfile>flowlogix-repository</activeProfile>
    </activeProfiles>

    <profiles>
        <profile>
            <id>payara-arquillian-version-override</id>
            <properties>
                <arquillian.payara5.version>2.5-lp-apr13</arquillian.payara5.version>
            </properties>
        </profile>

        <profile>
            <id>flowlogix-repository</id>
            <repositories>
                <repository>
                    <id>flowlogix-repository</id>
                    <url>https://nexus.flowlogix.com/repository/maven-releases</url>
                    <releases>
                        <enabled>true</enabled>
                        <checksumPolicy>fail</checksumPolicy>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
            </repositories>
        </profile>
    </profiles>
</settings>
